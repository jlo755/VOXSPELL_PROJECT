<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="124.0" prefWidth="406.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell_dialog.dialogController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="124.0" prefWidth="352.0" VBox.vgrow="ALWAYS">
      <children>
        <Label fx:id="output" alignment="CENTER" layoutX="141.0" layoutY="33.0" style="&#10;" text="Are you sure?" textAlignment="CENTER" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Button fx:id="yes" layoutX="110.0" layoutY="83.0" onAction="#yesAction" mnemonicParsing="false" prefHeight="26.0" prefWidth="62.0" text="Yes" />
            <Button fx:id="no" layoutX="235.0" layoutY="83.0" onAction="#noAction" mnemonicParsing="false" prefHeight="26.0" prefWidth="62.0" text="No" />
      </children>
    </AnchorPane>
  </children>
</VBox>
