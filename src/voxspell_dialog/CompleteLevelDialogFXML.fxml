<VBox prefHeight="211.0" prefWidth="426.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell_dialog.CompleteLevelDialogController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="215.0" prefWidth="426.0" VBox.vgrow="ALWAYS">
      <children>
        <Label fx:id="congratLabel" alignment="CENTER" layoutX="37.0" layoutY="14.0" prefHeight="106.0" prefWidth="353.0" style="&#10;" text="Congratulations you earned xxx coins for completing this level! \nYou now have: xxx coins." textAlignment="CENTER" wrapText="true">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Button fx:id="confirm" layoutX="178.0" layoutY="152.0" onAction="#confirmAction" mnemonicParsing="false" text="Confirm" />
      </children>
    </AnchorPane>
  </children>
</VBox>
