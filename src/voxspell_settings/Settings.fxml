<VBox prefHeight="446.0" prefWidth="371.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell_settings.SettingsController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="446.0" prefWidth="450.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="confirm" layoutX="77.0" layoutY="400.0" mnemonicParsing="false" onAction="#confirmAction" text="Confirm" />
            <Button fx:id="cancel" layoutX="231.0" layoutY="400.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
            <Label layoutX="148.0" layoutY="21.0" text="Settings">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <ScrollPane layoutX="35.0" layoutY="53.0" prefHeight="332.0" prefWidth="322.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="448.0" prefWidth="300.0">
                     <children>
                        <ComboBox fx:id="spellingLevelCombo" layoutX="79.0" layoutY="222.0" prefWidth="150.0" />
                        <Label layoutX="105.0" layoutY="200.0" text="Spelling Level" />
                        <Slider fx:id="volume" layoutX="77.0" layoutY="99.0" prefHeight="16.0" prefWidth="150.0" />
                        <Label layoutX="127.0" layoutY="76.0" text="Volume" />
                        <ComboBox fx:id="bgmCombo" layoutX="77.0" layoutY="30.0" prefWidth="150.0" />
                        <Label layoutX="136.0" layoutY="9.0" text="BGM" />
                        <ComboBox fx:id="voicesCombo" layoutX="77.0" layoutY="153.0" prefWidth="150.0" />
                        <Label layoutX="129.0" layoutY="131.0" text="Voices" />
                        <Label layoutX="120.0" layoutY="267.0" text="Select File" />
                        <TextField fx:id="fileField" layoutX="81.0" layoutY="291.0" prefHeight="26.0" prefWidth="150.0" />
                        <Button fx:id="uploadFile" layoutX="104.0" layoutY="328.0" mnemonicParsing="false" onAction="#uploadFileAction" prefHeight="26.0" prefWidth="101.0" text="Upload File" />
                     </children>
                  </AnchorPane>
              </content>
            </ScrollPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
