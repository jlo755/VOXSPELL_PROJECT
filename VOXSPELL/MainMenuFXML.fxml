<VBox prefHeight="400.0" prefWidth="640.0" stylesheets="@mainMenuStyle.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell_project.MainMenuController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="newGame" layoutX="283.0" layoutY="217.0" mnemonicParsing="false" onAction="#newGameAction" stylesheets="@mainMenuStyle.css" text="New Game" />
            <Button fx:id="visitShop" layoutX="283.0" layoutY="254.0" mnemonicParsing="false" onAction="#visitShopAction" prefHeight="26.0" prefWidth="90.0" text="Visit Shop" />
            <Button fx:id="exitGame" layoutX="283.0" layoutY="290.0" mnemonicParsing="false" onAction="#exitGameAction" prefHeight="26.0" prefWidth="90.0" text="Exit Game" />
            <Button layoutX="564.0" layoutY="14.0" mnemonicParsing="false" text="Settings" />
            <Button layoutX="474.0" layoutY="15.0" mnemonicParsing="false" text="Statistics" />
            <Label layoutX="87.0" layoutY="18.0" text="Master Speller" />
            <ProgressBar layoutX="87.0" layoutY="38.0" prefHeight="18.0" prefWidth="76.0" progress="1.0" />
            <ImageView id="imageUser" fx:id="imageView" fitHeight="66.0" fitWidth="63.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" smooth="false">
               <image>
                  <Image url="@dog.png" />
               </image>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</VBox>