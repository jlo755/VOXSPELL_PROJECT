<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="434.0" prefWidth="694.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell_spelling.NewGameController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextArea editable="false" layoutX="36.0" layoutY="86.0" opacity="0.94" prefHeight="219.0" prefWidth="621.0">
               <font>
                  <Font name="Sawasdee" size="37.0" />
               </font>
            </TextArea>
            <Label fx:id="wordLabel" alignment="TOP_CENTER" layoutX="36.0" layoutY="108.0" prefHeight="157.0" prefWidth="621.0" text="Word 1 of 5:" textAlignment="CENTER">
               <font>
                  <Font name="Sawasdee" size="37.0" />
               </font>
            </Label>
            <TextField fx:id="userSpell" layoutX="277.0" layoutY="182.0" onKeyPressed="#textFieldEnterAction" />
            <Button fx:id="stats" layoutX="518.0" layoutY="14.0" onAction="#statsAction" mnemonicParsing="false" text="Statistics" />
            <Button fx:id="settings" layoutX="608.0" layoutY="13.0" onAction="#settingsAction" mnemonicParsing="false" text="Settings" />
            <Button fx:id="back" layoutX="18.0" layoutY="15.0" mnemonicParsing="false" onAction="#backAction" text="Back" />
            <Button fx:id="submit" layoutX="330.0" layoutY="239.0" mnemonicParsing="false" onAction="#submitAction" text="Submit" />
            <Button fx:id="start" layoutX="337.0" layoutY="182.0" mnemonicParsing="false" onAction="#startAction" text="Start" />
      </children>
    </AnchorPane>
  </children>
</VBox>
