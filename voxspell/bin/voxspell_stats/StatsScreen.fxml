<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="471.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="voxspell_stats.StatsController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="442.0" prefWidth="416.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="186.0" fitWidth="282.0" layoutX="63.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true" />
            <Separator layoutX="-2.0" layoutY="274.0" prefHeight="4.0" prefWidth="412.0" />
            <Label fx:id="userLabel" layoutX="191.0" layoutY="311.0" text="User:" />
            <Label fx:id="wordsCorrectLabel" layoutX="191.0" layoutY="338.0" text="Words Correct:" />
            <Label fx:id="wordsAttemptLabel" layoutX="191.0" layoutY="368.0" text="Words Attempted:" />
            <Label fx:id="accuracyLabel" layoutX="192.0" layoutY="397.0" text="Accuracy Rating:" />
            <ImageView fitHeight="130.0" fitWidth="145.0" layoutX="30.0" layoutY="303.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../.resources/dog.png" />
               </image></ImageView>
            <Button fx:id="back" layoutX="32.0" layoutY="14.0" onAction="#backAction" mnemonicParsing="false" text="Back" />
         </children>
    </AnchorPane>
  </children>
</VBox>
